{
  "companySlug": "flutch",
  "name": "simple",
  "title": "Simple Conversational Graph",
  "description": "Basic conversational graph with optional reflection capabilities",
  "detailedDescription": "Multi-version simple graph supporting both basic conversation (v1.0.0) and enhanced processing with reflection capabilities (v2.0.0). Ideal for straightforward AI responses and thoughtful conversation workflows.",

  "category": "basic",
  "author": "Amelie Team",
  "maintainer": "Amelie Graph Team",
  "repository": "amelie-monorepo/packages/graphs/simple",
  "tags": ["simple", "conversation", "basic", "reflection", "multi-version"],
  "visibility": "public",

  "ui": {
    "enabled": false,
    "title": "Simple Graph",
    "description": "Basic conversational AI graph",
    "defaultScreen": "settings",
    "menu": ["settings"],
    "screens": {
      "settings": {
        "title": "Graph Settings",
        "type": "form",
        "fields": [
          {
            "name": "systemPrompt",
            "type": "textarea",
            "label": "System Prompt",
            "required": true
          },
          {
            "name": "temperature",
            "type": "number",
            "label": "Temperature",
            "required": false
          }
        ]
      }
    },
    "theme": {
      "primaryColor": "#28a745",
      "accentColor": "#6c757d"
    },
    "permissions": {
      "read": ["USER", "ADMIN"],
      "write": ["USER", "ADMIN"]
    }
  },

  "versioning": {
    "strategy": "semver",
    "defaultVersion": "2.0.0",
    "supportedVersions": ["1.0.0", "2.0.0"]
  },

  "versions": {
